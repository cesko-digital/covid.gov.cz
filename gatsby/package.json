{
  "name": "@covid/frontend",
  "private": true,
  "description": "Frontend vládního webu pro srozumitelný přehled informací a opatření ohledně pandemie koronaviru.",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev:local": "gatsby develop -H 0.0.0.0",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint ./src/**/*",
    "stylelint": "stylelint \"**/*.scss\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit",
    "postinstall": "cd gatsby-source-drupal && yarn link && cd .. && yarn link  gatsby-source-drupal"
  },
